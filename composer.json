{
    "name": "phlexible/elastica-bundle",
    "description": "phlexible elastica bundle",
    "license": "proprietary",
    "type": "symfony-bundle",
    "keywords": ["elastica", "elasticsearch"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\ElasticaBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.3.0",
        "ruflin/elastica": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "symfony/config": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/http-kernel": "~2.3",
        "symfony/yaml": "~2.3",
        "psr/log": "~1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
