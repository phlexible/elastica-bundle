{
    "name": "phlexible/elastica-bundle",
    "description": "phlexible elastica bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["elastica", "elasticsearch"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\ElasticaBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "ruflin/elastica": "^5.2.1|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "symfony/config": "^2.8",
        "symfony/dependency-injection": "^2.8",
        "symfony/http-kernel": "^2.8",
        "symfony/yaml": "^2.8",
        "psr/log": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
