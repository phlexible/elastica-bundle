{
    "name": "phlexible/elastica-bundle",
    "description": "phlexible elastica bundle",
    "license": "proprietary",
    "type": "symfony-bundle",
    "keywords": ["elastica", "elasticsearch"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\ElasticaBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.3.0",
        "ruflin/elastica": "~1.4"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.brainbits.net/repo/private/",
            "options": {
                "ssl": {
                    "allow_self_signed": "true"
                }
            }
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
